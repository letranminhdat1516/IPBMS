{
  "name": "healthcare_vision_backend",
  "version": "3.8.2",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:ci": "jest --runInBand --coverage --watchAll=false",
    "start": "node -r module-alias/register dist/src/main.js",
    "start:prod": "node -r module-alias/register dist/src/main.js",
    "start:dev": "ts-node-dev -r tsconfig-paths/register --watch --respawn --transpile-only --files --ignore-watch node_modules --no-notify --clear --max-old-space-size=4096 src/main.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "lint:quiet": "eslint . --ext .ts,.js --quiet",
    "type-check": "tsc --noEmit",
    "clean": "pnpm rimraf dist coverage",
    "prebuild": "pnpm clean && pnpm db:generate",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "prisma:seed": "TS_NODE_PROJECT=tsconfig.json TS_NODE_CWD=. ts-node -O '{\"module\":\"CommonJS\"}' ./prisma/seed/seed.ts",
    "prepare": "husky",
    "quality:fix": "pnpm lint:fix && pnpm format",
    "ci": "pnpm quality && pnpm test:ci && pnpm build",
    "pre-commit": "pnpm quality:fix && pnpm test --silent && pnpm build"
  },
  "keywords": [
    "healthcare",
    "prisma",
    "nestjs",
    "typescript",
    "postgresql"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/platform-socket.io": "^11.1.6",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.6",
    "@prisma/client": "^6.17.1",
    "@supabase/supabase-js": "^2.54.0",
    "@vonage/server-sdk": "^3.24.0",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cloudinary": "^1.41.3",
    "dayjs": "^1.11.13",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "fast-deep-equal": "^3.1.3",
    "firebase-admin": "^13.4.0",
    "helmet": "^8.1.0",
    "html-entities": "^2.6.0",
    "module-alias": "^2.2.3",
    "moment-timezone": "^0.6.0",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0",
    "mysql2": "^3.14.3",
    "nestjs-vnpay": "^1.0.4",
    "nodemailer": "^7.0.5",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^5.8.0",
    "typeorm": "^0.3.25",
    "uuid": "^13.0.0",
    "vnpay": "^2.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.34.0",
    "@jest/globals": "^30.1.2",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/testing": "^11.1.6",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/html-entities": "^1.3.4",
    "@types/jest": "^29.5.14",
    "@types/module-alias": "^2.0.4",
    "@types/moment-timezone": "^0.5.30",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.2.1",
    "@types/nodemailer": "^7.0.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "knip": "^5.66.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "prisma": "^6.17.1",
    "rimraf": "^6.0.1",
    "socket.io-client": "^4.8.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "prisma": {
    "seed": "node -r ts-node/register ./prisma/seed/seed.ts"
  },
  "_moduleAliases": {
    "@": "dist/src"
  },
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b"
}
